window.addEventListener('load', function(e) {
  document.querySelector('#test').innerHTML = 'coin flip';
}, false);

var Coin = {
    sideup: "heads",
    coinflip: function () {
        if (Math.floor(Math.random() + 1) == 0) {
            Coin.sideup = "heads";
        } else {
            Coin.sideup = "tails";
        }
        return Coin.sideup;
    }
};

var coin1 = Coin;

window.alert(coin1.coinflip());
